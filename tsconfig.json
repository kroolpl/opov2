{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "astro",
    "types": ["astro/client", "node"],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true
  }
}
